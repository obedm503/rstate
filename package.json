{
  "name": "rstate",
  "version": "0.1.0",
  "description": "Library description",
  "main": "src/rstate.ts",
  "scripts": {
    "lint":"tslint -c tslint.json 'src/**/*.ts'",
    "build": "tsc && tsc --outDir ./dist/lib/ -m commonjs && rollup -c",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "start": "npm run tsc",
    "pretest": "tsc -p test/tsconfig.json",
    "test": "mocha --opts test/mocha.opts",
    "tsc": "tsc --newLine LF --watch",
    "watch": "nodemon --watch src -e ts --exec 'tsc && rollup -c'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com:obedm503/rstate.git"
  },
  "keywords": [
    "library",
    "typescript"
  ],
  "author": "obedm503",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/obedm503/rstate/issues"
  },
  "homepage": "https://github.com/obedm503/rstate",
  "dependencies": {
    "dot-prop-immutable": "^1.3.1",
    "rxjs": "^5.4.2"
  },
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.21",
    "chai": "^3.5.0",
    "conventional-changelog-cli": "^1.3.1",
    "mocha": "^3.4.1",
    "nodemon": "^1.11.0",
    "rollup": "^0.41.6",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^3.2.2",
    "tslint": "^5.5.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.3.2"
  }
}
