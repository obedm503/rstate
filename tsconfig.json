{
  "compilerOptions": {
    "module": "es2015",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "noImplicitAny": false,
    "outDir": "./es/",
    "rootDir": "./src",
    "sourceMap": false,
    "inlineSourceMap": true,
    "inlineSources": true,
    "removeComments": true,
    "declaration": true,
    "noResolve": false,
    "moduleResolution": "node",
    "lib": [
      "es2015"
    ],
    "typeRoots": [
      "./node_modules/@types"
    ],
    "types": [
      "node"
    ]
  },
  "files": [
    "src/index.ts",
    "src/createStore.ts",
    "src/combineHandlers.ts",
    "src/util.ts"
  ],
  "atom": {
    "rewriteTsconfig": false
  },
  "compileOnSave": true
}
